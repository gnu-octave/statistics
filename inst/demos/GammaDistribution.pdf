%!demo
%! ## Plot various PDFs from the Gamma distribution
%! x = 0:0.01:10;
%! pd1 = makedist ("Gamma", "a", 1, "b", 1);
%! pd2 = makedist ("Gamma", "a", 2, "b", 1);
%! pd3 = makedist ("Gamma", "a", 5, "b", 1);
%! y1 = pdf (pd1, x);
%! y2 = pdf (pd2, x);
%! y3 = pdf (pd3, x);
%! plot (x, y1, "-b", x, y2, "-g", x, y3, "-r")
%! grid on
%! legend ({"a = 1, b = 1", "a = 2, b = 1", "a = 5, b = 1"}, ...
%!         "location", "northeast")
%! title ("Gamma PDF")
%! xlabel ("Values")
%! ylabel ("Probability density")
%!
%! ## This visualizes the probability density function for Gamma
%! ## distributions, showing the likelihood of different values.
