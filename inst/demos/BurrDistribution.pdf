%!demo
%! ## Plot various PDFs from the Burr distribution
%! x = 0:0.01:5;
%! pd1 = makedist ("Burr", "alpha", 1, "c", 2, "k", 1);
%! pd2 = makedist ("Burr", "alpha", 1, "c", 3, "k", 1);
%! pd3 = makedist ("Burr", "alpha", 1, "c", 4, "k", 1);
%! y1 = pdf (pd1, x);
%! y2 = pdf (pd2, x);
%! y3 = pdf (pd3, x);
%! plot (x, y1, "-b", x, y2, "-g", x, y3, "-r")
%! grid on
%! legend ({"alpha=1, c=2, k=1", "alpha=1, c=3, k=1", "alpha=1, c=4, k=1"}, ...
%!         "location", "northeast")
%! title ("Burr PDF")
%! xlabel ("values in x")
%! ylabel ("Probability density")
%!
%! ## This visualizes the probability density function for Burr distributions,
%! ## showing the likelihood of different values, e.g., income levels.
